{
  "name": "website",
  "description": "website",
  "author": "jared henderson <jared@netrivet.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run develop",
    "develop": "npx gatsby develop --port 2288",
    "ts:check": "npx tsc --noEmit",
    "lint": "npx eslint . --ext ts,tsx",
    "build": "npx gatsby build",
    "analyze": "ANALYZE_BUNDLE_SIZE=true npm run build",
    "format": "npx prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "serve": "npx gatsby serve",
    "upgrade-deps": "npx npm-check-updates -u && npm install"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.10.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.3",
    "eslint-plugin-react-hooks": "4.1.2",
    "gatsby": "2.24.67",
    "gatsby-image": "2.4.20",
    "gatsby-plugin-eslint": "2.0.8",
    "gatsby-plugin-manifest": "2.4.33",
    "gatsby-plugin-offline": "3.2.30",
    "gatsby-plugin-postcss": "3.0.0",
    "gatsby-plugin-purgecss": "5.0.0",
    "gatsby-plugin-react-helmet": "3.3.12",
    "gatsby-plugin-typescript": "2.4.21",
    "gatsby-source-filesystem": "2.3.32",
    "gatsby-plugin-webpack-bundle-analyzer": "1.0.5",
    "prettier": "2.1.2",
    "tailwindcss": "1.8.10",
    "typescript": "4.0.3"
  }
}
